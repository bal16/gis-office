# THIS IS AN EXAMPLE NOT AN ACTUAL ENV/CONFIGURATION

APP_NAME="GIS-OFFICE"
APP_ENV=local
APP_KEY= #generated using `php artisan key:generate`
APP_DEBUG=true
APP_TIMEZONE=Asia/Jakarta
APP_URL=http://localhost:8000
APP_EMAIL=@gis.dev # change this with your office email

APP_LOCALE=id # change to "en" for english
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK="single, stderr"
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite # change to your favorite db providers e.g. "mysql"
# uncomment the following lines if you want to use mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=public # change it to "local" if you want to use local storage and "s3" if you want to use aws
QUEUE_CONNECTION=sync #change to database if you want to use "job type" queue via artisan queue:work
# QUEUE_CONNECTION=sync # database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log # change to "smtp" if you want to use smtp
MAIL_SCHEME=null # change to tls if you want to use tls
MAIL_HOST=127.0.0.1 # change to your smtp server
MAIL_PORT=1025 # or 2525
MAIL_USERNAME=null # change to your smtp username
MAIL_PASSWORD=null # change to your smtp password
MAIL_FROM_ADDRESS="no-reply${APP_EMAIL}" # change to your email
MAIL_FROM_NAME="${APP_NAME}"

# uncomment the following lines if you want to use aws
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1
# AWS_BUCKET=
# AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
